.tab-bar{
  bottom: 0;
  display: table;
  width: 100%;
  height: 100rpx;
  padding: 0;
  table-layout: fixed;
  border-bottom: 0;
  position: fixed;
  z-index: 10;
  right: 0;
  left: 0;
  background-color: white;
  border-top:1px solid #D8D8D8;
}

.tab-on{
  color: black;
  display: table-cell;
  overflow: hidden;
  width: 1%;
  height: 100rpx;
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.tab-off{
  color: black;
  display: table-cell;
  overflow: hidden;
  width: 1%;
  height: 100rpx;
  text-align: center;
  vertical-align: middle;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.tab-label-off{
  font-size: 28rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #9B9B9B;
}

.tab-label-on{
  font-size: 28rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #42BD55;
}

.tab-icon{
  top:6rpx;
  width: 52rpx;
  height: 52rpx;
  padding-top: 0;
  padding-bottom: 0;
  font-size: 48rpx;
  position: relative;
  z-index: 20;
  margin-bottom: 7rpx;
}


.door-time{
  font-size: 32rpx;
  margin-bottom: 25rpx;
  color: #42BD55;
}

.night{
  text-align: center;
  color: #9B9B9B;
  font-size: 28rpx;
  margin-top: 75rpx;
}

.index-how{
  position: fixed;
  bottom: 0;
  background-color: #42BD55;
  color: white;
  font-size: 30rpx;
  text-align: center;
  height: 62rpx;
  padding-top: 30rpx;
  width: 100%;
}

.start-time{
  font-size: 34rpx;
  margin-top: 55rpx;
  margin-bottom: 55rpx;
}